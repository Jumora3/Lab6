<div id="<%= dom_id subject %>">
  <p>
    <strong>Name:</strong>
    <%= subject.name %>
  </p>

  <p>
    <strong>Section:</strong>
    <%= subject.section %>
  </p>

  <p>
    <strong>Number of units:</strong>
    <%= subject.number_of_units %>
  </p>

  <p>
    <strong>Per unit rate:</strong>
    <%= subject.per_unit_rate %>
  </p>

  <p>
    <strong>Number of students:</strong>
    <%= subject.number_of_students %>
  </p>
<p>
  <strong>Attendance:</strong>
  <%=subject.each do |student|%>
  <%subject.student.name%>
  <%end%>
  </p>
  <p>
    <strong>Teacher:</strong>
    <%= subject.teacher_id %>
  </p>

</div>
